Table dim_athlete {
    athlete_id integer [primary key, note: 'Surrogate key, autogenerated in dbt']
    athlete_first_name varchar(50) [not null]
    athlete_last_name varchar(50) [not null]
    athlete_date_of_birth date [not null]
    body_weight_kg decimal(5,2) [not null]
    gender varchar(10) [not null]
    created_at timestamp [default: 'now()']
    updated_at timestamp [default: 'now()']
    }

Table dim_date { 
    date_id integer [primary key, note: 'Surrogate key, autogenerated in dbt']
    full_date date [not null]
    year integer [not null]
    quarter integer [not null]
    month integer [not null]
    day integer [not null]
    week_number integer [not null]
    is_weekend boolean [not null]
    }

Table dim_exercise {
    exercise_id integer [primary key, note: 'Surrogate key, autogenerated in dbt']
    exercise_name varchar(50) [not null]
    muscle_group_primary varchar(50) [not null]
    muscle_group_secondary varchar(50)
    muscle_group_tertiary varchar(50)
    }
    
Table fact_strength {
    athlete_id integer [ref: > dim_athlete.athlete_id, not null]
    date_id integer [ref: > dim_date.date_id, not null]
    exercise_id integer [ref: > dim_exercise.exercise_id, not null]
    set_number integer [not null]
    reps integer [not null]
    weight_kgs decimal(5,2) [not null]
    extra_weight_kgs decimal(5,2)
    time_minutes integer
    volume_kg decimal(10,2) [not null, note: 'autogenerated in dbt as reps * (weight_kgs + extra_weight_kgs)']
    total_volume_session decimal(10,2) [not null, note: 'autogenerated in dbt as sum of volume of all workouts in the day']
    }
